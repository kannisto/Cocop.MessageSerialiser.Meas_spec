  <!--
    In this file: a response to a request to update a task.
  -->
  
  <swes:extension xsi:type="swe:DataRecordPropertyType">
    <swe:DataRecord>
      <swe:identifier>cocop/requestresult</swe:identifier>
      <swe:field name="result">
        <swe:Category>
          <swe:value>Ok</swe:value>
        </swe:Category>
      </swe:field>
      <swe:field name="message">
        <swe:Text>
          <swe:value></swe:value>
        </swe:Text>
      </swe:field>
    </swe:DataRecord>
	</swes:extension>
  
  <sps:result>
    <sps:StatusReport>
      <sps:task>mytask-1</sps:task>
      <sps:procedure>myproc-1</sps:procedure>
      <sps:requestStatus>Accepted</sps:requestStatus>
      <sps:taskStatus>InExecution</sps:taskStatus>
      <sps:updateTime>2020-01-01T11:00:00Z</sps:updateTime>
      <sps:taskingParameters>
        <sps:ParameterData>
          <sps:encoding>
            <swe:XMLEncoding />
          </sps:encoding>
          <sps:values xsi:type="swe:DataRecordPropertyType">
            <swe:DataRecord>
              <swe:field name="SomeCount1">
                <swe:Count>
                  <swe:value>3</swe:value>
                </swe:Count>
              </swe:field>
              <swe:field name="SomeCategory1">
                <swe:Category>
                  <swe:value>active</swe:value>
                </swe:Category>
              </swe:field>
            </swe:DataRecord>
          </sps:values>
        </sps:ParameterData>
      </sps:taskingParameters>
    </sps:StatusReport>
  </sps:result>
  <sps:targetTask>mytask-1</sps:targetTask>
</sps:UpdateResponse>
